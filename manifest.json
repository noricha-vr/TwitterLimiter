{
	"manifest_version": 3,
	"name": "Twitter Limiter",
	"version": "1.1",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"permissions": [
		"activeTab",
		"storage"
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "options.html",
		"default_icon": {
			"16": "icon.png",
			"48": "icon.png",
			"128": "icon.png"
		}
	},
	"host_permissions": [
		"https://twitter.com/*"
	]
}
