{
	"manifest_version": 3,
	"name": "Twitter Limiter",
	"version": "1.4",
	"icons": {
		"128": "icon/128.png"
	},
	"permissions": [
		"activeTab",
		"storage"
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "options.html",
		"default_icon": {
			"128": "icon/128.png"
		}
	},
	"host_permissions": [
		"https://twitter.com/*"
	]
}
